{
    "name": "PDF Accessibility Amazon Textract",
    "version": "v0.0.0",
    "actions": [
        {
            "title": "AutoTag (Textract)",
            "name": "pdfix_autotag_textract",
            "desc": "Automatically tags PDF using Amazon Textract [Cloud]",
            "version": "v0.0.0",
            "icon": "autotag_textract",
            "category": "Accessibility",
            "local": "False",
            "program": "docker run -v \"${working_directory}:/data\" -w /data --rm pdfix/autotag-textract:latest tag --name \"${license_name}\" --key \"${license_key}\" -i \"/data/${input_pdf}\" -o \"/data/${output_pdf}\" --aws_id \"${aws_id}\" --aws_secret \"${aws_secret}\" --aws_region \"${aws_region}\" --zoom \"${zoom}\"",
            "args": [
                {
                    "name": "input_pdf",
                    "desc": "Input PDF file",
                    "flags": 2,
                    "type": "file_path",
                    "ext": "pdf",
                    "value": ""
                },
                {
                    "name": "output_pdf",
                    "desc": "Output PDF file",
                    "flags": 4,
                    "type": "file_path",
                    "ext": "pdf",
                    "value": ""
                },
                {
                    "title": "AWS Access Key ID",
                    "name": "aws_id",
                    "desc": "AWS Access Key ID",
                    "type": "string",
                    "value": ""
                },
                {
                    "title": "AWS Secret Access Key",
                    "name": "aws_secret",
                    "desc": "AWS Secret Access Key",
                    "type": "string",
                    "value": ""
                },
                {
                    "title": "AWS Region",
                    "name": "aws_region",
                    "desc": "AWS Region with Textract service",
                    "type": "string",
                    "value": "us-east-1",
                    "set": [
                        {
                            "value": "ap-northeast-2"
                        },
                        {
                            "value": "ap-south-1"
                        },
                        {
                            "value": "ap-southeast-1"
                        },
                        {
                            "value": "ap-southeast-2"
                        },
                        {
                            "value": "ca-central-1"
                        },
                        {
                            "value": "eu-central-1"
                        },
                        {
                            "value": "eu-west-1"
                        },
                        {
                            "value": "eu-west-2"
                        },
                        {
                            "value": "eu-west-3"
                        },
                        {
                            "value": "us-east-1"
                        },
                        {
                            "value": "us-east-2"
                        },
                        {
                            "value": "us-gov-east-1"
                        },
                        {
                            "value": "us-gov-west-1"
                        },
                        {
                            "value": "us-west-1"
                        },
                        {
                            "value": "us-west-2"
                        }
                    ]
                },
                {
                    "title": "Rendering Zoom Level",
                    "name": "zoom",
                    "desc": "Pages are provided as images that are rendered using this zoom level",
                    "type": "float",
                    "value": "2.0"
                }
            ]
        },
        {
            "title": "Create Layout Template JSON (Textract)",
            "name": "pdfix_create_layout_template_json_textract",
            "desc": "Automatically creates layout template json using Amazon Textract, saving it as JSON file [Cloud]",
            "version": "v0.0.0",
            "icon": "rtt",
            "category": "Accessibility",
            "subtype": "create_layout_template_pdf_to_json",
            "local": "False",
            "program": "docker run -v \"${working_directory}:/data\" -w /data --rm pdfix/autotag-textract:latest template --name \"${license_name}\" --key \"${license_key}\" -i \"/data/${input_pdf}\" -o \"/data/${output_json}\" --aws_id \"${aws_id}\" --aws_secret \"${aws_secret}\" --aws_region \"${aws_region}\" --zoom \"${zoom}\"",
            "args": [
                {
                    "name": "input_pdf",
                    "desc": "Input PDF file",
                    "flags": 2,
                    "type": "file_path",
                    "ext": "pdf",
                    "value": ""
                },
                {
                    "name": "output_json",
                    "desc": "Output JSON file that is template json",
                    "flags": 4,
                    "type": "file_path",
                    "ext": "json",
                    "value": ""
                },
                {
                    "title": "AWS Access Key ID",
                    "name": "aws_id",
                    "desc": "AWS Access Key ID",
                    "type": "string",
                    "value": ""
                },
                {
                    "title": "AWS Secret Access Key",
                    "name": "aws_secret",
                    "desc": "AWS Secret Access Key",
                    "type": "string",
                    "value": ""
                },
                {
                    "title": "AWS Region",
                    "name": "aws_region",
                    "desc": "AWS Region with Textract service",
                    "type": "string",
                    "value": "us-east-1",
                    "set": [
                        {
                            "value": "ap-northeast-2"
                        },
                        {
                            "value": "ap-south-1"
                        },
                        {
                            "value": "ap-southeast-1"
                        },
                        {
                            "value": "ap-southeast-2"
                        },
                        {
                            "value": "ca-central-1"
                        },
                        {
                            "value": "eu-central-1"
                        },
                        {
                            "value": "eu-west-1"
                        },
                        {
                            "value": "eu-west-2"
                        },
                        {
                            "value": "eu-west-3"
                        },
                        {
                            "value": "us-east-1"
                        },
                        {
                            "value": "us-east-2"
                        },
                        {
                            "value": "us-gov-east-1"
                        },
                        {
                            "value": "us-gov-west-1"
                        },
                        {
                            "value": "us-west-1"
                        },
                        {
                            "value": "us-west-2"
                        }
                    ]
                },
                {
                    "title": "Rendering Zoom Level",
                    "name": "zoom",
                    "desc": "Pages are provided as images that are rendered using this zoom level",
                    "type": "float",
                    "value": "2.0"
                }
            ]
        }
    ]
}
